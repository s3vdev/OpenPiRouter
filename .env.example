# OpenPiRouter Configuration
# Copy this file to .env and fill in your values

# Dashboard Login
DASHBOARD_PASSWORD=your_secure_password_here

# Pi-hole Admin Password
PIHOLE_PASSWORD=your_pihole_password_here

# Web Interface Port
WEB_PORT=8080

# Default Network Configuration
DEFAULT_AP_SSID=OpenPiRouter
DEFAULT_AP_PASSWORD=your_wifi_password_here
DEFAULT_AP_CHANNEL=36
DEFAULT_AP_BAND=a

# Regional Settings (adjust for your country)
WIFI_COUNTRY=DE
TIMEZONE=Europe/Berlin

# System
HOSTNAME=openpirouter
